{
  "id": "reverb",
  "name": "Reverb Profissional",
  "description": "Simula a acústica de um espaço, adicionando profundidade e ambiente.",
  "category": "Ambiente",
  "ui": {
    "htmlPath": "effects/reverb/reverb.html",
    "jsPath": "effects/reverb/reverb.js"
  },
  "parameters": {
    "mix": { "name": "Mix (Wet/Dry)", "type": "slider", "min": 0, "max": 1, "step": 0.01, "defaultValue": 0.5 },
    "decay": { "name": "Decay (s)", "type": "slider", "min": 0.1, "max": 10, "step": 0.1, "defaultValue": 2.5 },
    "preDelay": { "name": "Pre-Delay (ms)", "type": "slider", "min": 0, "max": 200, "step": 1, "defaultValue": 30 },
    "bypass": { "name": "Bypass", "type": "toggle", "defaultValue": false }
  },
  "presets": [
    { "name": "Small Room", "mix": 0.3, "decay": 0.8, "preDelay": 10 },
    { "name": "Medium Hall", "mix": 0.5, "decay": 2.5, "preDelay": 30 },
    { "name": "Large Plate", "mix": 0.7, "decay": 4.0, "preDelay": 50 },
    { "name": "Cathedral", "mix": 0.8, "decay": 7.0, "preDelay": 80 }
  ]
}