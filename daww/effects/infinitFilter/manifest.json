{
  "id": "infinitFilter",
  "name": "infinitFilter",
  "description": "Filtro dinâmico com LFO para criar movimento e texturas. Ideal para efeitos de 'wobble' e 'wah-wah'.",
  "category": "Filtro",
  "ui": {
    "htmlPath": "effects/infinitFilter/infinitFilter.html",
    "jsPath": "effects/infinitFilter/infinitFilter.js"
  },
  "parameters": [
    { "id": "type", "name": "Tipo", "type": "select", "options": ["lowpass", "highpass", "bandpass"], "defaultValue": "lowpass" },
    { "id": "frequency", "name": "Frequência (Hz)", "type": "slider", "min": 20, "max": 10000, "step": 1, "defaultValue": 800 },
    { "id": "q", "name": "Ressonância (Q)", "type": "slider", "min": 0.1, "max": 20, "step": 0.1, "defaultValue": 1 },
    { "id": "lfo_rate", "name": "Velocidade LFO (Hz)", "type": "slider", "min": 0.1, "max": 20, "step": 0.1, "defaultValue": 5 },
    { "id": "lfo_depth", "name": "Profundidade LFO", "type": "slider", "min": 0, "max": 5000, "step": 10, "defaultValue": 0 },
    { "id": "mix", "name": "Mix (Wet/Dry)", "type": "slider", "min": 0, "max": 1, "step": 0.01, "defaultValue": 1 }
  ]
}